
<div class="modal fade" id="ReSetPWModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form name="RetrievePW" action="RetrievePW" method="post">
                <div class="modal-header">
                    <button type="button" class="close" Data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">重置登录密码</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label>新密码</label>
                        <input name="newPassword" id="f_password" class="form-control" placeholder="请输入新密码" type="password" />
                    </div>
                    <div class="form-group">
                        <label>确认新密码</label>
                        <input name="re_password" id="re_password" class="form-control" placeholder="确认新密码" type="password" />
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" onclick="ReSetPW()">确认修改</button>
                    <button type="button" class="btn btn-default" Data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>