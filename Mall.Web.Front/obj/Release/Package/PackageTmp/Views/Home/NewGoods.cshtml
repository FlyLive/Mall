
@model List<Mall.Data.DataBase.GoodsInfo>

<div class="suggest-part">
    <div class="suggest-left">
        <p class="suggest-title-strong">新<span>书上架</span></p>
        <div class="suggest-left-translate"></div>
        <div class="suggest-content">
            @Html.Partial("Home/_Suggest", Model.Take(4).ToList())
        </div>
    </div>
    <div class="suggest-advertisement">
        @Html.Partial("Home/_SuggestAdvertisement", @*Model[5]*@new Mall.Data.DataBase.GoodsInfo())
    </div>
</div>