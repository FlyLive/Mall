.head {
    width:1500px;
    display:flex;
    margin:0 auto;
}
.navbar-head{
    background-color:#222222;
}
a{
    text-decoration:none;
}
.head-message{
    font-size:20px;
}
.head-message input{
    text-indent:5px;
    font-size: 25px;
    height:40px;
    margin: 15px 5px;
    border-radius:5px;
    border: 1px solid #666666;
}
#search-button {
    cursor:pointer;
    height:40px;
    width:40px;
    border:none;
    background-image: url('../Pictures/Shared/Serch(UnTouch).png');
    background-color:rgba(0, 0, 0, 0.00);
    background-size:40px;
}
#search-button:hover {
    background-image: url('../Pictures/Shared/Serch(Touch).png');
}
.login,.menu,.head-message{
    color:#999999;
}
.logo .location{
    margin:0px 100px 0px 100px;
}
.logo img{
    vertical-align:middle;
    margin:10px 20px 10px 0px;
    height:50px;
}
.logo span{
    color:#999999;
}
.menu{
    margin-top:5px;
}
.menu li{
    list-style:none;
    float:left;
    padding:0px 10px 0px 10px;
}
.menu li a{
    color: #999999;
}
.menu li a img{
    position: relative;
    top: 5px;
    height:20px;
}